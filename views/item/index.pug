extends ../layout

block content
    for brand in brands
        img(src=brand.image style='width: 100px;')
        a(href=brand.url) #{brand.name}
        br
        
    br
    br
    for category in categories 
        a(href=category.url) #{category.name} 
        br
    br
    br
    for item in items 
        a(href=item.url) #{item.name} 
        p #{item.description} 
        p #{item.brand.name} 
        p #{item.price}
